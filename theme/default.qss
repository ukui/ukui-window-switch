QWidget#indicator_sub_widget {
    background-color: rgba(0, 0, 0, 0);
    margin: 0;
    border-style: none;
    border-width: 0px;
    padding: 0px;
}

UkwsIndicator#indicator_main_widget_switch {
    margin: 0;
    background-color: rgba(23, 23, 23, 176);
    border-radius: 6px;
    border-style: none;
    border-width: 0px;
    padding: 0px;
}

UkwsIndicator#indicator_main_widget_tiling {
    margin: 0;
/*    background-color: rgba(23, 23, 23, 176);*/
    border-radius: 0px;
    border-style: none;
    border-width: 0px;
    padding: 0px;
}

QWidget#ws_manager_sub_widget {
    border-radius: 1px;
    border-style: none;
    border-width: 0px;
    padding: 0px;
}

QWidget#ws_manager_main_widget {
    border-style: none;
    border-width: 0px;
    padding: 0px;
}

UkwsWindowExtraLabel#winbox-wintitle {
    font-size: 14px;
    color: white;
    margin: 0px, 0px, 0px, 5px;
    border-style: none;
    border-width: 0px;
    padding: 0px, 0px, 0px, 3px;
}

UkwsWindowExtraLabel#winbox-thumbnail:hover {
    margin: 0px;
    border: 4px solid rgba(255, 255, 255, 255);
    border-radius: 9px;
    padding: 0px;
}

UkwsWindowExtraLabel#winbox-thumbnail:!hover {
    margin: 0px;
    border: 4px solid rgba(255, 255, 255, 0);
    border-radius: 9px;
    padding: 0px;
}

